{{> common-header }} 
{{> common-search }}
<div class="tab-content" id="tabs-matches">
  <div class="tab-pane fade show active" id="matches-list" role="tabpanel" aria-labelledby="home-tab">
    <div id="landing-matches">
      <div class="section-matches">
        <div class="space-60"></div>
        <div class="container">
          <div class="row">
            <div class="col-md-12">
              <div class="panel-profile">
                <div class="panel-inner-left">
                  {{> common-matched-profile }}
                </div>
                <div class="panel-inner-right bg-white-f8">
                  {{> common-profile-before-conversation }}
                </div>
              </div>
              <div class="space-40"></div>
              <div class="panel-button">
                <ul class="list-inline m-0">
                  <li class="list-inline-item">
                    <a href="#" class="btn btn-sm white-three"><img  style="width:15px;" src="/images/icons-solid/left-arrow.svg" alt=""></a>show previous member
                  </li>
                  <li class="list-inline-item">
                    <a href="#" class="btn btn-sm white-three"><img  style="width:15px;" src="/images/icons-solid/right-arrow.svg" alt=""></a>show next member
                  </li>
                  <li class="list-inline-item">
                    <a style="width: 60px !important" href="#" class="btn btn-sm white-three">SPACE</a>give a high five
                  </li> 
                  <li class="list-inline-item">
                    <a href="#" class="btn btn-sm white-three"><img  style="width:17px;" src="/images/icons-solid/enter.svg" alt=""></a>send a message
                  </li>
                </ul>
              </div>
              <div class="space-40"></div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="tab-pane fade" id="messages" role="tabpanel" aria-labelledby="profile-tab">
  </div>
</div>