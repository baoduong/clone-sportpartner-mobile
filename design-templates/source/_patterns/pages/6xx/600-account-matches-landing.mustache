{{> common-header }} 
{{> common-search }}
<div class="tab-content" id="tabs-matches">
  <div class="tab-pane fade show active" id="matches-list" role="tabpanel" aria-labelledby="home-tab">        
    <div id="landing-matches">
      <div class="section-matches">
        <div class="space-60"></div> 
      </div>
      <!-- starting a page 1 -->
      <div class="pagging">
        <div class="section-matches">          
          <div class="container">
            <div class="row">
              <div class="col-md-12">
                <div class="match-items-panel">
                  {{# matchedItemsTop}} {{> common-matched-item }} {{/ matchedItemsTop}}
                </div>
              </div>
            </div>
          </div>
          <div class="space-20"></div>
        </div>
        <div class="section-matches">
          <div class="container-fluid">
            <div class="row">
              <div class="col-md-12">
                <div class="get-premium-panel">
                  <div class="space-30"></div>
                
                    <div class="d-block clearfix pl-5 pr-5">
                        {{> elements-close-button }}
                      </div>
                      <div class="space-10"></div>
                      <h2 class="m-0 text-center font-family-gilroy font-weight-bold font-size-40 text-color-white">Get the most out of Sportpartner. Go Premium</h2>
                      <div class="space-20"></div>
                      <div class="text-center">                      
                        <p class="ma-0 text-center text-color-white font-weight-300" style="opacity: 0.5;">Premium members have 50% more chance to find their partner within a day!</p>
                      </div>
                      <div class="space-40"></div>
                      <div class="text-center">
                        <a class="btn btn-round royal-purple" href="#">Get Premium today</a>                      
                      </div>
                    
               
                  <div class="space-60"></div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="section-matches">
          <div class="space-40"></div>    
          <div class="container">
            <div class="row">
              <div class="col-md-12">
                <div class="match-items-panel">
                  {{# matchedItemsBottom}} {{> common-matched-item }} {{/ matchedItemsBottom}}
                </div>
              </div>
            </div>
          </div>        
        </div>
      </div>
      <!-- ending a page 1-->       
      <!-- starting a page 2, just split up 2 pages for example, you delete when start biz code-->
      <div class="pagging">
        <div class="section-matches">          
          <div class="container">
            <div class="row">
              <div class="col-md-12">
                <div class="match-items-panel">
                  {{# matchedItemsTop}} {{> common-matched-item }} {{/ matchedItemsTop}}
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="section-matches">
          <div class="container-fluid">
            <div class="row">
              <div class="col-md-12">
                <div class="get-premium-panel">
                  <div class="space-30"></div>
                  <div class="row">
                    <div class="col-md-12">
                      <div class="d-block clearfix pl-5 pr-5">
                        {{> elements-close-button }}
                      </div>
                      <div class="space-10"></div>
                      <h2 class="m-0 Heading-1">Get the most out of Sportpartner. Go Premium</h2>
                      <div class="space-20"></div>
                      <div class="text-center">                      
                        <label class="ma-0 Paragraph-1-center">Premium members have 50% more chance to find their partner within a day!</label>
                      </div>
                      <div class="space-40"></div>
                      <div class="text-center">
                        <a class="btn btn-round btn-link black-two" style="width: 360px;" href="#">Get Premium today</a>                      
                      </div>
                    </div>
                  </div>
                  <div class="space-60"></div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="section-matches">
          <div class="space-20"></div>    
          <div class="container">
            <div class="row">
              <div class="col-md-12">
                <div class="match-items-panel">
                  {{# matchedItemsBottom}} {{> common-matched-item }} {{/ matchedItemsBottom}}
                </div>
              </div>
            </div>
          </div>        
        </div>
      </div>
      <!-- ending a page -->      
    </div>    
  </div>
</div>

<div id="header-purple"></div>